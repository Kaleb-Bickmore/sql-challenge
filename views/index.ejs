<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Index</title>
    <h1> Blogs for days</h1>
  </head>
  <body>
    <div>
    </div>
<form name="blogs" method="post">
  <textarea name="input"placeholder="write your blog post here"
  style="
  width:200px;
    height:100px";></textarea>
  <button>post</button>
  <div name="userBlogs">
  <% user.forEach(function(user){ %>
  <h2 ><%user.title%></h2>
  <h3>by +<%user.name%><h3>
  <p><%user.content%></p>
  <button href="./edit/<%user.name:user.id%>" >edit</button>
 <% }); %>
  </div>
</form>
  </body>
  <footer>
    <ul href="./views/edit">edit</ul>
    <ul href="./views/new.ejs">new</ul>
    <ul href="./views/index.ejs">index</ul>
  </footer>
</html>
